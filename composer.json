{
    "name": "jinom/user-service-sdk",
    "description": "Laravel SDK for User Service with authentication and auto user sync",
    "keywords": [
        "jinom",
        "laravel",
        "user-service",
        "sso",
        "oauth",
        "user-sync"
    ],
    "homepage": "https://github.com/jinom/user-service-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Jinom",
            "email": "dev@jinom.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "illuminate/support": "^10.0||^11.0||^12.0",
        "illuminate/http": "^10.0||^11.0||^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.0.0",
        "pestphp/pest": "^3.0||^4.0",
        "pestphp/pest-plugin-arch": "^3.0||^4.0",
        "pestphp/pest-plugin-laravel": "^3.0||^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Jinom\\UserServiceSdk\\": "src/",
            "Jinom\\UserServiceSdk\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jinom\\UserServiceSdk\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jinom\\UserServiceSdk\\UserServiceSdkServiceProvider"
            ],
            "aliases": {
                "UserServiceSdk": "Jinom\\UserServiceSdk\\Facades\\UserServiceSdk"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
